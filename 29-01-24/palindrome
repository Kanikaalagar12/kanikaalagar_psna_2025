class Solution {
    public boolean isPalindrome(int x) {
        if (x < 0) {
            return false; 
        }
        int y=0,b;
        int temp=x;
        while(temp!=0){
           b= temp % 10;
            y=y*10+b;
            temp=temp/10;
            
        }
        
          return x==y;

        }
    }
